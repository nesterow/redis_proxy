{
  "name": "redisproxy",
  "version": "1.0.0",
  "description": "A Resis Proxy",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/main.js",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/main.ts",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && NODE_ENV=test ts-mocha -p tsconfig.json src/**/*.spec.ts",
    "test:e2e": "ts-mocha -p tsconfig.json test/**/*.spec.ts"
  },
  "author": "nesterow",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "redis": "^2.8.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/expect": "^1.20.4",
    "@types/express": "^4.17.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.1",
    "@types/redis": "^2.8.14",
    "@types/uuid": "^3.4.5",
    "expect": "^24.9.0",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.3",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  }
}
